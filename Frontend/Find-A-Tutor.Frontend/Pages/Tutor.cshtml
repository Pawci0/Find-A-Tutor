@page
@model Find_A_Tutor.Frontend.Pages.TutorModel
@{
    ViewData["Title"] = "Tutor";
}

<h1>Tutor</h1>

    @if (Model.privateLessons.Value.Count() > 0 && Model.privateLessons.IsSuccess)
    {
        foreach (var item in Model.privateLessons.Value)
        {
            <table>
                <tr>
                    <td>@item.Id</td>
                    <td>@item.StudentId</td>
                    <td>@item.TutorId</td>
                    <td>@item.CreatedAt</td>
                    <td>@item.TakenAt</td>
                    <td>@item.UpdatedAt</td>
                    <td>@item.RelevantTo</td>
                    <td>@item.Description</td>
                    <td>@item.Subject</td>
                    <td>@item.IsAssigned</td>
                    <td>@item.IsPaid</td>
                    <td>@item.IsDone</td>
                </tr>
            </table>
        }
    }
    else
    {
        <p>Brak lekcji</p>
    }


